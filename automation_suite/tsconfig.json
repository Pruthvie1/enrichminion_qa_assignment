{
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "noEmit": true,
    "jsx": "react",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "resolveJsonModule": true,
    "types": [
      "@playwright/test"
    ]
  },
  "include": [
    "tests/**/*.ts"
  ]
}